<div class="container-fluid">
<div class="row" id="login">
      <div class="col-md-12 login">



<h1>Log in, Fool!</h1>

<%= form_for @user, :url => { :controller => "sessions", :action => "create" } do |f| %>

  <div class="field">
    email<%= f.text_field :email %><br>
   password <%= f.password_field :password %> 
  </div>
  <div class="actions">
    <%= f.submit "Log me in!" %>
  </div>
  <% end %>
  </div>
  </div>
  </div>